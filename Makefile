CC = gcc
CFLAGS = -Wall -g

all: marvin

marvin: chatsvr.o util.o trickyserver.o lookup.o parse.o testparse.o
        gcc -g -o marvin chatsvr.o util.o trickyserver.o lookup.o       parse.o testparse.o

clean:
        rm -f marvin marvin.o chatsvr.o util.o trickyserver.o lookup.o  parse.o testparse.o


# these lines generated by "gcc -MM *.c"
util.o: util.c util.h
trickyserver.o: trickyserver.c chatsvr.c
lookup.o: lookup.c
parse.o: parse.c parse.h
testparse.o: testparse.c






